<aiml>

	<category>
		<pattern>_ HEY</pattern>
		<template>
		    UNDERLINE IS <star index="1" />
		</template>
	</category>

	<category>
		<pattern>* HEY</pattern>
		<template>
		    STAR IS <star index="1" />
		</template>
	</category>

	<category>
		<pattern>HELLO _</pattern>
		<template>
		    UNDERLINE IS <star index="1" />
		</template>
	</category>

	<category>
		<pattern>HELLO *</pattern>
		<template>
		    STAR IS <star index="1" />
		</template>
	</category>

	<category>
		<pattern>HI _ MATE</pattern>
		<template>
		    UNDERLINE IS <star index="1" />
		</template>
	</category>

	<category>
		<pattern>HI * MATE</pattern>
		<template>
		    STAR IS <star index="1" />
		</template>
	</category>

</aiml>
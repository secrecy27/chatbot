<aiml>

	<category>
		<pattern>MAKE EXPLODE</pattern>
		<template>
			<explode>hello world</explode>
		</template>
	</category>

	<category>
		<pattern>MAKE IMPLODE</pattern>
		<template>
			<implode>hello world</implode>
		</template>
	</category>

	<category>
		<pattern>NESTED EXPLODE IMPLODE</pattern>
		<template>
			<implode><explode>hello world</explode></implode>
		</template>
	</category>

	<category>
		<pattern>NESTED IMPLODE EXPLODE</pattern>
		<template>
			<explode><implode>hello world</implode></explode>
		</template>
	</category>

</aiml>